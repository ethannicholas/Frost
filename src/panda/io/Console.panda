package panda.io

@final
class Console {
    @class
    @external
    method print(c:Char8)

    @class
    method print(s:String) {
        for i in 0 .. s.size {
            Console.print(s.data[i])
        }
    }

    @class
    method print(o:Object) {
        print(o.convert())
    }

    @class
    method printLine(o:Object) {
        print(o.convert())
        printLine()
    }

    @class
    method printLine() {
        print(Char8(10))
    }
}