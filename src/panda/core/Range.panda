package panda.core

class Range<T> : Value {
    init(start:T, end:T, step:Int?, inclusive:Bit) {
        self.start := start
        self.end := end
        if step != null {
            self.step := step
        }
        else {
            self.step := 1
        }
        self.inclusive := inclusive
    }

    def start:T

    def end:T

    def step:Int

    def inclusive:Bit
}