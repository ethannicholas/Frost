package panda.core

class Object {
    function =(o:Object):Bit {
        return self == o
    }

    function !=(o:Object):Bit {
        return !(self = o)
    }

    function hash():Int {
        return 0
    }

    function convert():String {
        return "<object>"
    }

    @protected
    method cleanup() {
    }

    @private
    def $class:Class

    @private
    var refCount:Int32
}