package org.frostlang.frostc

@final
class Alias : Symbol {
    def type:Type

    init(alias:String, type:Type) {
        super.init(Kind.ALIAS, type.position, alias)
        self.type := type
    }

    @override
    function get_toString():String {
        return("Alias(\{name}->\{type})")
    }
}