package org.frostlanguage.plex

class DFAState {
    def id:Int
    def label:DFAStateLabel
    var scanned := false

    init(id:Int, label:DFAStateLabel) {
        self.id := id
        self.label := label
    }

    @override
    function convert():String {
        return "DFAState<\{id}, \{label}>"
    }
}