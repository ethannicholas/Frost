package org.pandalanguage.plex.runtime

@final
class RawToken : Value {
    var kind:Int
    var start:String.Index
    var end:String.Index
    var line:Int
    var column:Int

    init(kind:Int, start:String.Index, end:String.Index, line:Int, column:Int) {
        self.kind := kind
        self.start := start
        self.end := end
        self.line := line
        self.column := column
    }
}