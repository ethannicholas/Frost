method main() {
    try {
        def out := IndentedOutputStream(Console.outputStream())
        out.printLine("method main() {")
        out.level += 1
        out.printLine("def collection := [\"Hello\", \"Bonjour\", \"Hola\", \"Guten Tag\"]")
        Console.inputStream().sendTo(out)
        out.level -= 1
        out.printLine("}")
    }
    fail(error) {
        Console.printLine(error)
        System.exit(1)
    }
}