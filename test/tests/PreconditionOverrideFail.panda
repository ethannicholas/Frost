class PreconditionOverrideFail1 {
    @pre(x > 0)
    method test(x:Int) {
        Console.printLine("Got X1: \{x}")
    }
}

class PreconditionOverrideFail2 : PreconditionOverrideFail1 {
    @override
    @preOr(x < -10)
    method test(x:Int) {
        Console.printLine("Got X2: \{x}")
    }

    @class
    method main() {
        PreconditionOverrideFail2().test(1)
        PreconditionOverrideFail2().test(-12)
        PreconditionOverrideFail2().test(-9)
    }
}