class Point2D {
    init(x:Int64, y:Int64) {
        self.x := x
        self.y := y
    }

    var x:Int64
    var y:Int64
}

class Point3D : Point2D {
    init(x:Int64, y:Int64, z:Int64) {
        self.x := x
        self.y := y
        self.z := z
    }

    var z:Int64
}

class Untyped {
    init() {}
    
    var untyped := 12
}

class Fields {
    @class
    method main() {
        var p1 := Point2D(1, 2)
        print(p1.x)
        print(p1.y)
        
        var p2 := Point3D(5, 10, 15)
        print(p2.x)
        print(p2.y)
        print(p2.z)

        p1 := p2
        print(p1.x)
        print(p1.y)

        print(Untyped().untyped)
    }
}