method main() {
    def r :=  0 ... 13 by -1
    for i in r {
        Console.printLine(i)
    }

    def h := "Hello, World!"

    Console.printLine("+")
    Console.printLine(h + "!")
    Console.printLine(h + 5123)
    Console.printLine(h + true)
    Console.printLine(false + h)
    Console.printLine()

    Console.printLine("startsWith")
    Console.printLine(h.startsWith("Hello"))
    Console.printLine(h.startsWith("hello"))
    Console.printLine(h.startsWith(""))
    Console.printLine(h.startsWith("Hello, World!"))
    Console.printLine(h.startsWith("Hello, World! And Some More Stuff"))
    Console.printLine()

    Console.printLine("endsWith")
    Console.printLine(h.endsWith("!"))
    Console.printLine(h.endsWith(""))
    Console.printLine(h.endsWith("Hello, World!"))
    Console.printLine(h.endsWith("world!"))
    Console.printLine(h.endsWith("More Stuff Hello, World!"))
    Console.printLine()

    Console.printLine("[Range]")
    Console.printLine(h[..])
    Console.printLine(h[.. by 1])
    Console.printLine(h[.. by 2])
    Console.printLine(h[.. by -1])
    Console.printLine(h[.. by -2])
    Console.printLine(h[...])
    Console.printLine(h[... by 1])
    Console.printLine(h[... by 2])
    Console.printLine(h[... by -1])
    Console.printLine(h[... by -2])
    Console.printLine(""[..])
    Console.printLine(""[...])
    Console.printLine(""[.. by -1])
    Console.printLine(""[... by -1])
    Console.printLine(h[h.start .. h.offset(h.start, 5)])
    Console.printLine(h[h.start ... h.offset(h.start, 5)])
    Console.printLine(h[h.start ... h.offset(h.start, 5) by 2])
    Console.printLine(h[h.offset(h.start, 7)..])
    Console.printLine(h[h.offset(h.start, 7)...])
    Console.printLine(h[h.offset(h.start, 7) .. h.offset(h.start, 12)])
    Console.printLine(h[h.offset(h.start, 7) ... h.offset(h.start, 12)])
    Console.printLine(h[h.offset(h.start, 11) ... h.offset(h.start, 7) by -1])
    Console.printLine(h[h.offset(h.start, 4) .. by -1])
    Console.printLine(h[h.offset(h.start, 4) ... by -1])
    Console.printLine(h[h.start ... by 100])
    Console.printLine()

    Console.printLine("Empty[Range]")
    Console.printLine(""[..])
    Console.printLine(""[.. by 1])
    Console.printLine(""[.. by 2])
    Console.printLine(""[.. by -1])
    Console.printLine(""[.. by -2])
    Console.printLine(""[...])
    Console.printLine(""[... by 1])
    Console.printLine(""[... by 2])
    Console.printLine(""[... by -1])
    Console.printLine(""[... by -2])
    Console.printLine(""[0..])
    Console.printLine(""[..0 by 1])
    Console.printLine(""[0..0 by 2])
    Console.printLine(""[0.. by -1])
    Console.printLine(""[..0 by -2])
    Console.printLine(""[0...0])
    Console.printLine(""[0... by 1])
    Console.printLine(""[...0 by 2])
    Console.printLine(""[0... by -1])
    Console.printLine(""[0...0 by -2])
    Console.printLine()

    Console.printLine("indexOf")
    Console.printLine(h[h.indexOf("")])
    Console.printLine(h[h.indexOf("Hello")])
    Console.printLine(h[h.indexOf("!")])
    Console.printLine(h[h.previous(h.lastIndexOf(""))])
    Console.printLine(h[h.lastIndexOf("Hello")])
    Console.printLine(h[h.lastIndexOf("!")])
    Console.printLine(h[h.next(h.indexOf("l"))])
    Console.printLine(h[h.next(h.lastIndexOf("l"))])
    Console.printLine(h[h.previous(h.lastIndexOf("ll"))])
    Console.printLine(h.indexOf("Hello, World!!") == null)
    Console.printLine(h.indexOf("Q") == null)
    Console.printLine()

    Console.printLine("split1")
    def split1 := "This is a long string".split(" ")
    for i in 0 .. split1.count {
        Console.printLine(split1[i])
    }
    Console.printLine()

    Console.printLine("split2")
    def split2 := "This is a long string".split(" ", 3)
    for i in 0 .. split2.count {
        Console.printLine(split2[i])
    }
    Console.printLine()

    Console.printLine("replace")
    Console.printLine("Hello, World!".replace("o", "123"))
    Console.printLine("123456789".replace("0123", "456"))
    Console.printLine("123456789".replace("123", "456"))
    Console.printLine("123456789".replace("789", "456"))
    Console.printLine("aaaaaaaaa".replace("a", "b"))
    Console.printLine("aa".replace("aaa", "bbb"))
    Console.printLine("aa".replace("", "bbb"))
    Console.printLine("aa".replace("a", ""))
    Console.printLine()

    Console.printLine("split")
    Console.printLine("Hello/World!".split("/"))
    Console.printLine("--hello--world----test--".split("--"))
    Console.printLine("1,2,3,4,5,6".split(",", 2))
    Console.printLine("Hello".split("", 3))
    Console.printLine()

    Console.printLine("trim")
    Console.printLine("Hello, World!".trimmed)
    Console.printLine("  \n Hello, World! \r\t  ".trimmed)
    Console.printLine("     ".trimmed)
    Console.printLine("".trimmed)
    Console.printLine()

    Console.printLine("contains")
    Console.printLine("Hello, World!".contains("Hello"))
    Console.printLine("Hello, World!".contains("Helllo"))
    Console.printLine("Hello, World!".contains(" "))
    Console.printLine("Hello, World!".contains("!!"))
    Console.printLine("Hello, World!".contains(""))
    Console.printLine("Hello, World!".contains("Hello, World!!"))
    Console.printLine()

    Console.printLine("*")
    Console.printLine("Hello" * 3)
    Console.printLine("Hello" * 0)
    Console.printLine("" * 3)
    Console.printLine()

    Console.printLine("format")
    Console.printLine("Hello".format(""))
    Console.printLine("Hello".format("frost"))
    Console.printLine("\tHe\"llo\n\\".format("frost"))
}
