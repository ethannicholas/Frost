class Overloading {
    init() {
    }

    init(x:Int64) {
        print(-1)
    }

    @class
    method classTest() {
        print(1)
    }

    @class
    method classTest(x:Int64) {
        print(x)
    }

    @class
    method classTest(x:Int64, y:Int64) {
        print(x + y)
    }

    method instanceTest() {
        print(4)
    }

    method instanceTest(x:Int64) {
        print(x)
    }

    method instanceTest(x:Int64, y:Int64) {
        print(x + y)
    }

    @class
    method main() {
        classTest()
        classTest(2)
        classTest(4, -1)
        def o := Overloading()
        o.instanceTest()
        o.instanceTest(5)
        o.instanceTest(-12, 18)
    }
}