class RangeForLoop {
    @class
    method main() {
        Console.printLine("-128 ... 127")
        for i in (-128)->Int8 ... 127->Int8 {
            Console.printLine("    " + i)
        }
        Console.printLine("-128 .. 127")
        for i in (-128)->Int8 .. 127->Int8 {
            Console.printLine("    " + i)
        }
        Console.printLine("127 ... -128")
        for i in 127->Int8 ... (-128)->Int8 {
            Console.printLine("    " + i)
        }
        Console.printLine("127 ... -128 by -1")
        for i in 127->Int8 ... (-128)->Int8 by -1 {
            Console.printLine("    " + i)
        }
        Console.printLine("127 .. -128 by -1")
        for i in 127->Int8 .. (-128)->Int8 by -1 {
            Console.printLine("    " + i)
        }
        Console.printLine("-128 ... 127 by 10")
        for i in (-128)->Int8 ... 127->Int8 by 10 {
            Console.printLine("    " + i)
        }
        Console.printLine("127 ... -128 by -10")
        for i in 127->Int8 ... (-128)->Int8 by -10 {
            Console.printLine("    " + i)
        }
    }
}