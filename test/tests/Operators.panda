class Operators {
    @class
    method int64() {
        var x:Int64 := 10
        var y:Int64 := 7
        Console.printLine("Int64 Arithmetic")
        Console.printLine(x + y)
        Console.printLine(x - y)
        Console.printLine(x * y)
        Console.printLine(x // y)
        Console.printLine(x << 1)
        Console.printLine(x >> 1)
        Console.printLine("Int64 Comparisons")
        Console.printLine(x = y)
        Console.printLine(x = x)
        Console.printLine(x != y)
        Console.printLine(x != x)
        Console.printLine(x > x)
        Console.printLine(x > y)
        Console.printLine(y > x)
        Console.printLine(x >= y)
        Console.printLine(y >= x)
        Console.printLine(x >= x)
        Console.printLine(x < y)
        Console.printLine(y < x)
        Console.printLine(y < y)
        Console.printLine(x <= y)
        Console.printLine(y <= x)
        Console.printLine(y <= y)
        Console.printLine("Int64 Compound Assignment")
        x += y
        Console.printLine(x)
        x -= y
        Console.printLine(x)
        x *= y
        Console.printLine(x)
        x //= y
        Console.printLine(x)
        x <<= 1
        Console.printLine(x)
        x >>= 1
        Console.printLine(x)
    }

    @class
    method noisyTrue():Bit {
        Console.printLine(-1)
        return true
    }

    @class
    method noisyFalse():Bit {
        Console.printLine(-2)
        return false
    }

    @class
    method bit() {
        Console.printLine("Bit Logic")
        Console.printLine(!noisyTrue())
        Console.printLine(!noisyFalse())
        Console.printLine(noisyTrue() & noisyTrue())
        Console.printLine(noisyTrue() & noisyFalse())
        Console.printLine(noisyFalse() & noisyTrue())
        Console.printLine(noisyFalse() & noisyFalse())
        Console.printLine(noisyTrue() | noisyTrue())
        Console.printLine(noisyTrue() | noisyFalse())
        Console.printLine(noisyFalse() | noisyTrue())
        Console.printLine(noisyFalse() | noisyFalse())

        Console.printLine("Bit Compound Assignment")
        var x := true
        x &= noisyTrue()
        Console.printLine(x)

        x := true
        x &= noisyFalse()
        Console.printLine(x)

        x := false
        x &= noisyTrue()
        Console.printLine(x)

        x := false
        x &= noisyFalse()
        Console.printLine(x)

        x := true
        x |= noisyTrue()
        Console.printLine(x)

        x := true
        x |= noisyFalse()
        Console.printLine(x)

        x := false
        x |= noisyTrue()
        Console.printLine(x)

        x := false
        x |= noisyFalse()
        Console.printLine(x)
    }

    @class
    method main() {
        int64()
        bit()
    }
}