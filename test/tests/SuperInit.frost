class Super {
    init() {
        x := 123
    }

    init(x:Int) {
        self.x := x
    }

    var x:Int
}

class Sub1 : Super {
    init() {
        super.init(456)
    }
}

class Sub2 : Super {
}

class SuperInit {
    @class
    method main() {
        Console.printLine(Sub1().x)
        Console.printLine(Sub2().x)
    }
}