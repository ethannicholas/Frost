class StackTest {
    @class
    method main() {
        def s := Stack<Int?>()
        s.push(1)
        s.push(2)
        s.push(3)
        s.push(null)
        s.push(4)
        Console.printLine(s.peek())
        Console.printLine(s.pop())
        Console.printLine(s.peek(1))
        Console.printLine(s.pop() != null)
        Console.printLine(s.pop())
        Console.printLine(s.pop())
        Console.printLine(s.pop())
        Console.printLine(s.get_count())
    }
}